<?xml version="1.0" encoding="utf-8"?>
<ability_list
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="AbilityData.xsd">

	<!--Weasel XML |||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||-->

	<ability>
		<ability_ID>101</ability_ID>
		<ability_name>Scurry</ability_name>
		<ability_icon>5_56</ability_icon>
		<ability_data>
			<cd>10</cd>
			<duration>4</duration>
		</ability_data>
		<tooltip_list>
			<default>Go invisible and increase your movement speed by {movementSpeedMultiplier} times.</default>
		</tooltip_list>
		<targeting_list>
			<noneTargetingData/>
		</targeting_list>
		<var_list>
			<float name="movementSpeedMultiplier">1.5</float>
			<float name="damageMultiplier">2.0</float>
		</var_list>
		<ability_tags_to_apply>
			<tag>INVISIBLE</tag>
		</ability_tags_to_apply>
	</ability>

	<!--|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||-->

	<ability>
		<ability_ID>102</ability_ID>
		<ability_name>Chomp</ability_name>
		<ability_icon>6_09</ability_icon>
		<ability_data>
			<cd>1</cd>
		</ability_data>
		<tooltip_list>
			<default>Bite in front of you</default>
			<red>and stun</red>
			<blue>and hit twice</blue>
			<yellow>and reduce all cooldowns</yellow>
		</tooltip_list>
		<targeting_list>
			<lineTargetingData>
				<range>2</range>
				<width>1.3</width>
			</lineTargetingData>
		</targeting_list>
		<var_list>
			<float name="damage">5</float>
			<float name="hitboxDuration">0.25</float>
			<float name="hitboxRadius">0.5</float>
			<kb name="knockbackPreset">BIG</kb>
			<float name="stunDuration">1.0</float>
			<int name="multiHit">2</int>
			<float name="otherAbilityCooldownTick">1.0</float>
		</var_list>
	</ability>

	<!--|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||-->

	<ability>
		<ability_ID>103</ability_ID>
		<ability_name>Swipe</ability_name>
		<ability_icon>5_45</ability_icon>
		<ability_data>
			<cd>1</cd>
		</ability_data>
		<tooltip_list>
			<default>Swipe in a cone in front of you</default>
		</tooltip_list>
		<targeting_list>
			<lineTargetingData>
				<range>1</range>
				<width>2</width>
			</lineTargetingData>
		</targeting_list>
		<var_list>
			<float name="damage">20</float>
			<float name="hitboxDuration">0.25</float>
		</var_list>
	</ability>

	<!--|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||-->

	<ability>
		<ability_ID>104</ability_ID>
		<ability_name>Maul</ability_name>
		<ability_icon>1_21</ability_icon>
		<ability_data>
			<cd>1</cd>
			<doesTicking>true</doesTicking>
		</ability_data>
		<tooltip_list>
			<default>Swing multiple times in front of you</default>
		</tooltip_list>
		<targeting_list>
			<lineTargetingData>
				<range>1</range>
				<width>5</width>
			</lineTargetingData>
		</targeting_list>
		<var_list>
			<float name="damageInitial">10</float>
			<float name="damageFinal">20</float>
			<float name="hitboxDuration">0.25</float>
		</var_list>
		<ability_tags_to_apply>
			<tag>NORMAL_MOVEMENT_DISABLED</tag>
		</ability_tags_to_apply>
	</ability>

	<!--|||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||||-->
	
</ability_list>